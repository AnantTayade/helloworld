@model IEnumerable<StudentRegistration.Property.RegisterStudent>

@{
    ViewData["Title"] = "GetAllRecord";
}
<table class="table" border="1" style="margin-top:14px;">
    <thead>
        <tr>
            @if (Model.ToList().Count > 0)
            {
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Mobile)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EmailId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.OrganizationName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Address)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Country)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.State)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.City)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Pin)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TypeOfOrganization)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Other)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.OwnerName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.OwnerContactNo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.OwnerEmailId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.GSTNO)
                </th>
                <th></th>
            }
            else
            {
                <h3 style="color:darkslategrey;">Rcord NotFound!</h3>
            }
        </tr>
    </thead>
    <tbody>
        @{
            @if (Model != null)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Mobile)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EmailId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.OrganizationName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Address)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Country)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.State)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.City)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Pin)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TypeOfOrganization)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Other)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.OwnerName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.OwnerContactNo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.OwnerEmailId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.GSTNO)
                        </td>
                    </tr>
                }
            }
        }
    </tbody>
</table>
